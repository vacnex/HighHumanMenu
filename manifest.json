{
    "manifest_version": 2,
    "name": "HighHuman Menu",
    "version": "0.1",
    "permissions": [
        "contextMenus"
    ],
    "content_scripts": [
        {
            "matches": [
            "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}