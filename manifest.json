{
    "manifest_version": 2,
    "name": "HighHuman Menu",
    "version": "0.6",
    "icons": {
        "16": "assets/Icon/16.png",
        "48": "assets/Icon/48.png",
        "128": "assets/Icon/128.png"
    },
    "permissions": [
        "contextMenus",
        "tabs",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
            "<all_urls>"
            ],
            "js": ["assets/js/jquery.min.js", "content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "assets/settings.html"
    }
}