{
    "manifest_version": 2,
    "name": "HighHuman Menu",
    "version": "0.6",
    "permissions": [
        "contextMenus",
        "tabs",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
            "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "assets/settings.html"
    }
}